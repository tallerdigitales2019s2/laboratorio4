module DetectGameEndAux(positions_vector, win_o, winner_o);
input logic [1:0] positions_vector [0:2];
output logic win_o, winner_o;

logic all_equal, all_active;

xnor(all_equal , positions_vector[0][0], positions_vector[1][0], positions_vector[2][0]);
and(all_active, positions_vector[0][1], positions_vector[1][1], positions_vector[2][1]);
and(winner_o, all_equal,positions_vector[0][0]);
and(win_o, all_equal,all_active);


endmodule